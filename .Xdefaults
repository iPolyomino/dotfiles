! Rxvt
Rxvt*geometry:			80x35
Rxvt*font:			terminus-24
Rxvt*boldFont:			terminus-bold-24
Rxvt*loginShell:		on
Rxvt*visualBell:		on
Rxvt*scrollBar:			off
Rxvt*borderColor:		aquamarine4

! 0=black, 1=red,  2=green,  3=yellow,  4=blue, 5=magenta,  6=cyan,  7=white

urxvt*cursorColor: #ffffff

urxvt*background: #293134
urxvt*foreground: #E0E2E4

!! black colors
urxvt*color0:  #000000
urxvt*color8:  #666666
!! red colors
urxvt*color1:  #d54e53
urxvt*color9:  #ff3334

!! green dark/light
urxvt*color2:  #b9ca4a
urxvt*color10: #9ec400
 
!! yellow dark/light
urxvt*color3:  #e6c547
urxvt*color11: #e7c547
 
!! blue dark/light
urxvt*color4:  #7aa6da
urxvt*color12: #7aa6da
 
!! magenta dark/light
urxvt*color5:  #c397d8
urxvt*color13: #b77ee0
 
!! cyan dark/light
urxvt*color6:  #70c0ba
urxvt*color14: #70c0ba
 
!! white dark/light
urxvt*color7:  #ffffff
urxvt*color15: #ffffff

! Emacs
Emacs.geometry:			80x54
Emacs.foreground:		white
Emacs.background:		#172727
Emacs.cursorColor:		aquamarine1
Emacs.font:			terminus-18
Emacs.toolBar:			off
Emacs.menuBar:			off
Emacs.verticalScrollBars:	off
Emacs.reverseVideo:		off
